  <body>
    <div class="container">
    <h2>Livros Disponíveis</h2>
      <table class="table table-striped">
        <thead>
          <tr>
            <th>#</th>
            <th>Título</th>
            <th>Descrição</th>
            <th>Preço</th>
          </tr>
        </thead>
        <tbody>
          <% for(var i=0; i<lista.length;i++) {%>
            <tr> <label for=""></label>
                <td><%=lista[i].id%></td>
                <td><%=lista[i].titulo%></td>
                <td><%=lista[i].descricao%></td>
                <td><label for="">R$</label> <%=lista[i].preco%></td>
            </tr>
          <%}%>
          <tr>
          </tr>
        </tbody>

      </table>
    </div>


  </body>
</html>
